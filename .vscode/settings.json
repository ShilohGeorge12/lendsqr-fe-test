{
	"workbench.editor.customLabels.patterns": {
		"**/app/**/page.tsx": "${dirname} - Page",
		"**/app/**/index.tsx": "${dirname} - Index",
		"**/app/**/loading.tsx": "${dirname} - Loading",
		"**/app/**/error.tsx": "${dirname} - Error",
		"**/app/**/layout.tsx": "${dirname} - Layout",
		"**/components/**/index.tsx": "${dirname} - Component",
		"**/context/**/index.tsx": "Context API",
		"**/lib/**/index.tsx": "${dirname} - Service",
		"**/hooks/**/index.tsx": "${dirname} - Hook",
		"**/utils/**/index.tsx": "${dirname} - Util",
		"**/types/**/index.tsx": "Types"
	},
	"importSorter.sortConfiguration.customOrderingRules.defaultNumberOfEmptyLinesAfterGroup": 1,
	"importSorter.sortConfiguration.customOrderingRules.rules": [
		{
			"regex": "^next*",
			"orderLevel": 5
		},
		{
			"regex": "^react*",
			"orderLevel": 8
		},
		{
			"regex": "^@/lib/*",
			"orderLevel": 10
		},
		{
			"regex": "^@/hooks/*",
			"orderLevel": 20
		},
		{
			"regex": "^@/components/*",
			"orderLevel": 20
		},
		{
			"regex": "^@/types/*",
			"orderLevel": 30
		},
		{
			"regex": ".*",
			"orderLevel": 40
		}
	]
}
